{
  "manifest_version": 2,

  "name": "Robot take over the World!",
  "version": "1.0",
  "icons": {
    "256": "robot256.png"
  },

  "browser_action": {
    "default_icon": "robot256.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["program.js"]
  }]

}
 