{
    "manifest_version": 2,
    "name": "Add Background",
    "description": "Add Background to Google",
    "version": "1.0",
    "icons": {
        "260": "/images/robot.png",
        "350": "/images/Barbara.png",
        "400":"/images/shooter.jpg"
    },
    "browser_action": {
        "default_icon": {
            "260": "/images/robot.png",
            "350": "/images/Barbara.png",
            "400": "/images/shooter.jpg"
    } 
  },
  "content_scripts": [{
      "matches": ["https://www.google.com/*"],
      "css": ["main.css"]
  }]

}